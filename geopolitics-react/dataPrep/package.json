{
  "name": "geopolitics-react-data-prep",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc --watch && vite build --watch",
    "preview": "vite preview",
    "pretty": "prettier --write src && prettier --write out",
    "pretty:staged": "prettier --write $(git diff --name-only --diff-filter d | grep '\\.ts$' | xargs)",
    "runcli": "ts-node src/main.ts",
    "clean": "rm -rf out/*"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.3.1",
    "commander": "^11.0.0",
    "figlet": "^1.6.0",
    "prettier": "2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
